extends ../../templates/template.pug
include ../../utils/pageTerminal
include ../../utils/cardCity

block append variables
  - const CURRENT_PAGE = "terminal-norte"
  - const URL = "https://estelibuses.web.app"
  - const ASSETS_PATH = "../"
  - const HAS_CRITICAL_CSS = false
  - const APPLICATION_NAME = "Esteli Buses"
  - const META_DESCRIPTION = "Conoce todos los horarios de los buses de la Terminal Norte de la ciudad de Estelí."
  - const META_ROBOTS = "index, follow"
  - const LANGUAGE = "es"
  - const CURRENT_URL = URL
  - const SOCIAL_TITLE = "Horarios de la Terminal Norte de Buses de Estelí"
  - const SOCIAL_DESCRIPTION = META_DESCRIPTION
  - const SOCIAL_IMAGE = ""
  - const SOCIAL_IMAGE_ALT = SOCIAL_TITLE
  - const OPEN_GRAPH_SITE_NAME = ""
  - const TWITTER_CARD = "summary_large_image"

block append title
  title= SOCIAL_TITLE

block append preconnect

block append async-scripts

block append critical-css

block append stylesheets
  link(href=`${ASSETS_PATH}css/style.css` rel="stylesheet")/

block append preload

block append defer-scripts
  script(src=`${ASSETS_PATH}js/polyfill-lazyload.js` defer importance="low")
  script(src=`${ASSETS_PATH}js/search.js` defer importance="high")

block append content
  +pageTerminal('terminal-norte', 'Horarios de la terminal norte de Estelí', 'Conoce todos los horarios de la terminal norte de Estelí y elige la ciudad a la que deseas viajar.', 'Del centro comercial Erwin 30 metros al sur', '1234-5678')
    +cardCity('Jinotega', 'norte', 'data:image/png;base64,')

    +cardCity('Somoto', 'norte', 'data:image/png;base64,')

    +cardCity('Ocotal', 'norte', 'data:image/png;base64,')

    +cardCity('Yalí', 'norte', 'data:image/png;base64,')

    +cardCity('Quilalí', 'norte', 'data:image/png;base64,')

    +cardCity('San Juan de Limay', 'norte', 'data:image/png;base64,')

  article(class="py-12 sm:py-16 md:py-20 xl:py-28 text-center bg-light-100")
    div(class="container mx-auto px-4")
      h2(class="text-3xl sm:text-4xl font-bold mb-8") Conoce sobre la Terminal Norte de Estelí
      p(class="mx-auto mt-5 text-base sm:text-lg md:max-w-5xl lg:max-w-6xl mb-4") Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur.
      style.
        .aspect-ratio-tn {
          aspect-ratio: 736/982;
        }
        @media (min-width: 992px) {
          .aspect-ratio-tn {
            aspect-ratio: 1024/576;
          }
        }
      figure
        picture
          source(
            type="image/avif"
            data-srcset="/img/terminal-norte-estelibuses-mobile-felix-icaza.avif 912w, /img/terminal-norte-estelibuses-felix-icaza.avif"
          )/
          source(
            type="image/webp"
            data-srcset="/img/terminal-norte-estelibuses-mobile-felix-icaza.webp 912w, /img/terminal-norte-estelibuses-felix-icaza.webp"
          )/
          source(
            type="image/jpeg"
            data-srcset="/img/terminal-norte-estelibuses-mobile-felix-icaza.webp 912w, /img/terminal-norte-estelibuses-felix-icaza.jpg"
          )/
          img(
            alt="Terminal norte de la ciudad de Estelí"
            class="h-auto rounded object-cover object-center w-full max-w-5xl mx-auto lazyload aspect-ratio-tn"
            decoding="async"
            importance="low"
            loading="lazy"
            srcset
            data-srcset="/img/terminal-norte-estelibuses-mobile-felix-icaza.jpg 912w, /img/terminal-norte-estelibuses-felix-icaza.jpg"
          )/
        figcaption(class="text-sm text-gray-400 mt-3") Fachada de la Terminal Norte de Estelí.

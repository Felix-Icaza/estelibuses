extends ../../templates/template.pug
include ../../utils/pageTerminal
include ../../utils/cardCity

block append variables
  - const CURRENT_PAGE = "terminal-norte"
  - const URL = "https://estelibuses.web.app"
  - const ASSETS_PATH = "../"
  - const HAS_CRITICAL_CSS = false
  - const APPLICATION_NAME = "Estelí Buses"
  - const META_DESCRIPTION = "Conoce todos los horarios de los buses de la Terminal Norte de la ciudad de Estelí."
  - const META_ROBOTS = "index, follow"
  - const LANGUAGE = "es"
  - const CURRENT_URL = URL
  - const SOCIAL_TITLE = "Horarios de la Terminal Norte de Buses de Estelí"
  - const SOCIAL_DESCRIPTION = META_DESCRIPTION
  - const SOCIAL_IMAGE = ""
  - const SOCIAL_IMAGE_ALT = SOCIAL_TITLE
  - const OPEN_GRAPH_SITE_NAME = ""
  - const TWITTER_CARD = "summary_large_image"

block append title
  title= SOCIAL_TITLE

block append preconnect

block append async-scripts

block append critical-css

block append stylesheets
  link(href=`${ASSETS_PATH}css/style.css` rel="stylesheet")/

block append preload

block append defer-scripts
  script(src=`${ASSETS_PATH}js/search.js` defer importance="high")

block append content
  +pageTerminal('terminal-norte', 'Horarios de la terminal norte de Estelí', 'Conoce todos los horarios de la terminal norte de Estelí y elige la ciudad a la que deseas viajar.', 'Del Centro Comercial Erwin 30 metros hacia el sur', '', 'norte', 'Jinotega', 'Somoto', 'Ocotal', 'Yalí', 'Quilalí', 'San Juan de Limay', 'San Juan del Río Coco', 'La Rica', 'San Rafael del Norte', 'Wiwilí', 'El Regadío')
    +cardCity('Jinotega', 'norte', 'data:image/png;base64,')

    +cardCity('Somoto', 'norte', 'data:image/png;base64,')

    +cardCity('Ocotal', 'norte', 'data:image/png;base64,')

    +cardCity('Yalí', 'norte', 'data:image/png;base64,')

    +cardCity('Quilalí', 'norte', 'data:image/png;base64,')

    +cardCity('San Juan de Limay', 'norte', 'data:image/png;base64,')

    +cardCity('San Juan del Río Coco', 'norte', 'data:image/png;base64,')

    +cardCity('La Rica', 'norte', '', 'no-image')

    +cardCity('San Rafael del Norte', 'norte', 'data:image/png;base64,')

    +cardCity('Wiwilí', 'norte', 'data:image/png;base64,')

    +cardCity('El Regadío', 'norte', 'data:image/png;base64,')

  article(class="py-12 sm:py-16 md:py-20 xl:py-28 text-left sm:text-center bg-light-50")
    div(class="container mx-auto px-4")
      h2(class="text-3xl sm:text-4xl font-bold mb-8") Conoce sobre la Terminal Norte de Estelí
      div(class="space-y-4")
        p(class="mx-auto mt-5 text-base sm:text-lg md:max-w-5xl lg:max-w-6xl mb-4") La terminal norte o mejor conocida como&nbsp;
          em Cotrán Norte
          | &nbsp; está ubicada del Centro Comercial Erwin 30 metros hacia el sur, contiguo a la Carretera Panamericana.

        style.
          .aspect-ratio-tn {
            aspect-ratio: 544/726;
          }
          @media (min-width: 768px) {
            .aspect-ratio-tn {
              aspect-ratio: 1024/576;
            }
          }
        figure
          picture
            source(
              type="image/avif"
              data-srcset="/img/terminal-norte-estelibuses-mobile-felix-icaza.avif 600w, /img/terminal-norte-estelibuses-felix-icaza.avif"
            )/
            source(
              type="image/webp"
              data-srcset="/img/terminal-norte-estelibuses-mobile-felix-icaza.webp 600w, /img/terminal-norte-estelibuses-felix-icaza.webp"
            )/
            source(
              type="image/jpeg"
              data-srcset="/img/terminal-norte-estelibuses-mobile-felix-icaza.webp 600w, /img/terminal-norte-estelibuses-felix-icaza.jpg"
            )/
            img(
              alt="Terminal norte de la ciudad de Estelí"
              class="h-auto rounded object-cover object-center w-full max-w-5xl mx-auto lazyload aspect-ratio-tn"
              decoding="async"
              importance="low"
              loading="lazy"
              srcset="data:image/png;base64, 600w, data:image/png;base64,"
              data-srcset="/img/terminal-norte-estelibuses-mobile-felix-icaza.jpg 600w, /img/terminal-norte-estelibuses-felix-icaza.jpg"
            )/
          figcaption(class="text-sm text-gray-400 mt-3") Terminal Norte de la ciudad de Estelí.
    iframe#mapa(
      src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3886.2767920870906!2d-86.3543146846077!3d13.081636290783834!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x8f718c6c8f2298e7%3A0x855d282eb62cfeed!2sCotran%20Norte!5e0!3m2!1ses-419!2sni!4v1645728089801!5m2!1ses-419!2sni"
      allowfullscreen
      class="lazyload rounded w-full max-w-5xl mx-auto !mt-14"
      loading="lazy"
      style="border: 0; background: #eee"
      height="450"
    )

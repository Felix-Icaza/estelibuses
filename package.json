{
  "name": "estelibuses",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "author": {
    "name": "Felix Icaza",
    "email": "contacto@felixicaza.com",
    "url": "https://felixicaza.com/"
  },
  "scripts": {
    "analysis": "windicss-analysis",
    "build": "run-s -l -n pug windicss compress copyfiles compress:br",
    "build:test": "run-s -l -n clear build serve:test analysis",
    "clear": "rimraf public",
    "compress": "run-s -l -n compress:html postcss compress:js compress:img",
    "compress:br": "node brotli.js",
    "compress:html": "node html-minifier-terser.js",
    "compress:img": "node pngquant.js && node mozjpeg.js && cross-env NODE_ENV=production node webp.js",
    "compress:js": "node terser.js",
    "copyfiles": "node copy.js",
    "format": "run-s -l -n format:all format:pug format:xml",
    "format:all": "prettier --write \"./**/*.{js,json,md,yml}\"",
    "format:pug": "prettier --write \"./**/*.pug\" --plugin @prettier/plugin-pug",
    "format:xml": "prettier --write \"./**/*.xml\" --plugin @prettier/plugin-xml",
    "functions:deploy": "pnpm -r run deploy",
    "functions:emulators:start": "pnpm -r run emulators:start",
    "functions:logs": "pnpm -r run logs",
    "functions:shell": "pnpm -r run shell",
    "functions:start": "pnpm -r run start",
    "lint": "eslint --cache --fix --ext .js ./",
    "postcss": "postcss build/**/*.css --dir public/css/ --no-map --env production",
    "prepare": "husky install",
    "pug": "pug src/pages -o public",
    "pug:dev": "pug src/pages -o build -P -w",
    "serve": "run-p -l -n pug:dev windicss:dev serve:dev",
    "serve:dev": "servor build index.html 5000 --static",
    "serve:test": "servor public index.html 7000 --static",
    "wait": "wait-on build/index.html -d 7000",
    "windicss": "windicss public/**/*.{html,js} -tmo public/css/lib/windi.css -f windi.config.js",
    "windicss:dev": "run-s -l -n wait windicss:build",
    "windicss:build": "windicss build/**/*.{html,js} -tdo build/css/lib/windi.css -f windi.config.js"
  },
  "lint-staged": {
    "./**/*.js": [
      "pnpm lint",
      "prettier --write \"./**/*.js\""
    ],
    "./**/*.pug": "pnpm format:pug",
    "./build/css/*.css": "prettier --write \"./build/css/*.css\"",
    "./**/*.json": "prettier --write \"./**/*.json\"",
    "./**/*.xml": "pnpm format:xml",
    "./**/*.yml": "prettier --write \"./**/*.yml\"",
    "./**/*.md": "prettier --write \"./**/*.md\""
  },
  "dependencies": {
    "@windicss/plugin-animations": "1.0.9",
    "@windicss/plugin-question-mark": "0.1.1",
    "windicss": "3.4.3",
    "windicss-content-visibility": "1.1.0",
    "windicss-media-interaction": "1.1.1"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "1.19.2",
    "@prettier/plugin-xml": "1.2.0",
    "autoprefixer": "10.4.2",
    "cpy": "8.1.2",
    "cross-env": "7.0.3",
    "cssnano": "5.0.17",
    "cssnano-preset-advanced": "5.1.12",
    "eslint": "8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.2",
    "fast-glob": "3.2.11",
    "globby": "13.1.1",
    "html-minifier-terser": "6.1.0",
    "husky": "7.0.4",
    "imagemin": "8.0.1",
    "imagemin-mozjpeg": "10.0.0",
    "imagemin-pngquant": "9.0.2",
    "imagemin-webp": "7.0.0",
    "lint-staged": "12.3.4",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.6",
    "postcss-cli": "9.1.0",
    "postcss-momentum-scrolling": "3.14.22",
    "postcss-preset-env": "7.4.1",
    "postcss-pxtorem": "6.0.0",
    "postcss-sort-media-queries": "4.2.1",
    "postcss-sorting": "7.0.1",
    "postcss-urlrev": "2.0.0",
    "postcss-will-change-transition": "1.2.0",
    "prettier": "2.5.1",
    "pug-cli": "1.0.0-alpha6",
    "rimraf": "3.0.2",
    "servor": "4.0.2",
    "terser": "5.10.0",
    "wait-on": "6.0.1",
    "windicss-analysis": "0.3.5"
  },
  "engines": {
    "node": "^16.x",
    "npm": "^8.x"
  }
}

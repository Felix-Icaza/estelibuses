{
  "name": "estelibuses",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "license": "GPL-3.0",
  "author": {
    "name": "Felix Icaza",
    "email": "contacto@felixicaza.com",
    "url": "https://felixicaza.com/"
  },
  "scripts": {
    "analysis": "windicss-analysis",
    "build": "astro build",
    "build:test": "run-s -l -n clear build serve:test analysis",
    "clear": "rimraf public",
    "compress": "run-s -l -n compress:html postcss postcss:windi compress:js compress:img",
    "compress:br": "node brotli.js",
    "compress:html": "node html-minifier-terser.js",
    "compress:img": "node pngquant.js && node mozjpeg.js && cross-env NODE_ENV=production node webp.js",
    "compress:js": "node terser.mjs",
    "copyfiles": "node copy.js",
    "dev": "astro dev",
    "functions:deploy": "pnpm -r run deploy",
    "functions:serve": "pnpm -r run serve",
    "functions:logs": "pnpm -r run logs",
    "functions:shell": "pnpm -r run shell",
    "functions:start": "pnpm -r run start",
    "minify-manifest": "minify-json public/manifest.json",
    "optimize": "run-s -l -n optimize:pug optimize:html optimize:html:restore",
    "optimize:html": "rimraf build/**/*.html",
    "optimize:html:restore": "git restore build/google4ed26fc938fd8294.html",
    "optimize:pug": "rimraf src/pages/404.pug src/pages/acerca-de-estelibuses src/pages/anuncios src/pages/contacto src/pages/offline.pug src/pages/politica-de-privacidad src/pages/terminal-norte-esteli src/pages/terminal-sur-esteli",
    "optimize:restore": "git restore src/pages/404.pug src/pages/acerca-de-estelibuses src/pages/anuncios src/pages/contacto src/pages/offline.pug src/pages/politica-de-privacidad src/pages/terminal-norte-esteli src/pages/terminal-sur-esteli",
    "postcss": "postcss build/**/*.css --dir public/css/ --no-map --env production",
    "postcss:windi": "postcss public/css/lib/windi.css -o public/css/lib/windi.css --no-map --env production",
    "prepare": "husky install",
    "preview": "astro preview",
    "pug": "pug src/pages -o public",
    "pug:dev": "pug src/pages -o build -P -w",
    "serve": "run-p -l -n pug:dev windicss:dev serve:dev",
    "serve:dev": "servor build index.html 5000 --static",
    "serve:test": "servor public index.html 7000 --static",
    "start": "astro dev",
    "wait": "wait-on build/index.html -d 7000",
    "windicss": "windicss public/**/*.{html,js} -tmo public/css/lib/windi.css -f windi.config.js",
    "windicss:dev": "run-s -l -n wait windicss:build",
    "windicss:build": "windicss build/**/*.{html,js} -tdo build/css/lib/windi.css -f windi.config.js"
  },
  "lint-staged": {
    "./**/*.{js,mjs,cjs}": [
      "eslint --cache --fix --ext .js,.mjs,.cjs ./",
      "prettier --write \"./**/*.{js,mjs,cjs}\""
    ],
    "./src/**/*.astro": [
      "eslint --cache --ext .astro ./src/ --fix",
      "prettier --write \"./src/**/*.astro\" --plugin-search-dir=."
    ],
    "./src/styles/*.css": "prettier --write \"./src/styles/*.css\"",
    "./**/*.json": "prettier --write \"./**/*.json\"",
    "./**/*.xml": "prettier --write \"./**/*.xml\" --plugin @prettier/plugin-xml",
    "./**/*.md": "prettier --write \"./**/*.md\"",
    "./**/*.{yaml,yml}": "prettier --write \"./**/*.{yaml,yml}\""
  },
  "dependencies": {
    "@windicss/plugin-animations": "1.0.9",
    "@windicss/plugin-question-mark": "0.1.1",
    "astro": "1.4.6",
    "windicss": "3.4.3",
    "windicss-content-visibility": "1.2.0",
    "windicss-media-interaction": "1.1.1"
  },
  "devDependencies": {
    "@astrojs/tailwind": "2.0.1",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@prettier/plugin-pug": "2.0.0",
    "@prettier/plugin-xml": "2.1.0",
    "astro-eslint-parser": "0.7.2",
    "astro-seo-schema": "1.1.0",
    "autoprefixer": "10.4.5",
    "cpy": "8.1.2",
    "cross-env": "7.0.3",
    "cssnano": "5.1.7",
    "cssnano-preset-advanced": "5.3.3",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-astro": "0.20.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-n": "15.0.0",
    "eslint-plugin-promise": "6.0.0",
    "fast-glob": "3.2.11",
    "globby": "13.1.1",
    "html-minifier-terser": "6.1.0",
    "husky": "8.0.1",
    "imagemin": "8.0.1",
    "imagemin-mozjpeg": "10.0.0",
    "imagemin-pngquant": "9.0.2",
    "imagemin-webp": "7.0.0",
    "lint-staged": "13.0.3",
    "minify-json": "1.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.13",
    "postcss-cli": "9.1.0",
    "postcss-momentum-scrolling": "3.14.22",
    "postcss-preset-env": "7.4.4",
    "postcss-pxtorem": "6.0.0",
    "postcss-sort-media-queries": "4.2.1",
    "postcss-sorting": "7.0.1",
    "postcss-urlrev": "3.0.1",
    "postcss-will-change-transition": "1.2.0",
    "prettier": "2.7.1",
    "prettier-plugin-astro": "0.5.4",
    "pug-cli": "1.0.0-alpha6",
    "rimraf": "3.0.2",
    "servor": "4.0.2",
    "tailwindcss": "3.1.8",
    "terser": "5.13.1",
    "wait-on": "6.0.1",
    "windicss-analysis": "0.3.5"
  },
  "engines": {
    "node": "^16.x",
    "npm": "^8.x"
  }
}
